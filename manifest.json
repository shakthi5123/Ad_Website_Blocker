
{
    "name": "shakthi's Ad Blocker",
    "version": "1.0",
    "description": "lets get rid of the annoying adds",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "storage",
        "activeTab",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "icons":{
        "16": "icons/logo_16.png",
        "48": "icons/logo_48.png",
        "128": "icons/logo_128.png"
    },
    "manifest_version": 2
}
